{
  "name": "react-jwt-auth-redux",
  "version": "0.5.3",
  "description": "React JWT Auth (react-jwt-auth) wrappers for Redux",
  "repository": {
    "type": "git",
    "url": "https://github.com/fullstackforger/react-jwt-auth-redux"
  },
  "files": [
    "lib",
    "README"
  ],
  "main": "lib/cjs/index",
  "jsnext:main": "lib/es6/index",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --require ./internals/test.setup.js ./src/**/*.spec.js",
    "build": "npm run build-cjs && npm run build-es",
    "build-cjs": "rimraf lib && cross-env set BABEL_ENV=cjs && babel ./src -d lib/cjs --ignore **/*.spec.js",
    "build-es": "rimraf es6 && cross-env BABEL_ENV=es babel ./src -d lib/es6 --ignore **/*.spec.js",
    "prepublish": "npm test && npm run build",
    "preversion": "npm test",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "react",
    "auth",
    "jwt",
    "authentication",
    "token",
    "component",
    "flux",
    "redux"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "react-jwt-auth": "^0.5.8"
  },
  "peerDependencies": {
    "react": "^15.3.2"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^3.5.0",
    "cross-env": "^3.1.3",
    "fetch-mock": "^5.5.0",
    "jsdom": "^9.8.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-jwt-auth": "^0.5.4",
    "rimraf": "^2.5.4"
  }
}
